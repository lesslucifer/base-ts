{
  "name": "swmt",
  "version": "1.0.1",
  "description": "Smart water backend",
  "main": "dist/app.js",
  "scripts": {
    "int-test": "cross-env mocha -r node_modules/ts-node/register \"test/integration/**/*.ts\" --reporter list --exit",
    "test": "cross-env mocha -r node_modules/ts-node/register \"test/**/*.ts\" --reporter list --exit",
    "test-file": "cross-env mocha -r node_modules/ts-node/register --reporter list",
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts",
    "serve": "nodemon dist/app.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "coverage": "nyc mocha \"test/**/*.ts\"",
    "swagger": "ts-node tasks/submit_swagger_to_s3.ts"
  },
  "author": "VuLQ",
  "license": "ISC",
  "dependencies": {
    "async-lock": "^1.1.3",
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "ioredis": "^4.2.0",
    "jsonwebtoken": "^8.3.0",
    "minimist": "^1.2.0",
    "moment": "^2.22.1",
    "node-schedule": "^1.3.0",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.12",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "sha1": "^1.1.1",
    "typescript": "^2.9.1",
    "uuid": "^3.3.2",
    "xml": "^1.0.1"
  },
  "devDependencies": {
    "@types/async-lock": "^1.1.0",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.6",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-http": "^3.0.5",
    "@types/chai-spies": "^1.0.0",
    "@types/express": "^4.11.1",
    "@types/fs-extra": "^5.0.4",
    "@types/ioredis": "^4.0.4",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/lodash": "^4.14.108",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.0.2",
    "@types/node-schedule": "^1.2.2",
    "@types/randomstring": "^1.1.6",
    "@types/request-promise": "^4.1.41",
    "@types/sha1": "^1.1.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.0",
    "chai-spies": "^1.0.0",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1"
  }
}
